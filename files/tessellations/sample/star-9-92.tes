let(u = 9/2)

distunit(edge_angles(2*pi/v,pi/u,pi/u))

option("-switch-fpp", "-canvas-random", "0", "coolfog=0", "draw_sky=0", "fixed_YZ=0", "-PM", "0", "pseudohedral=1")

let(u7 = regangle(1, v))
unittile(u7,u7,u7,u7,u7,u7,u7,u7,u7)

