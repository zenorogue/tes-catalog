## {4,5} half-domino, grid 8
h2.
let(e4=edge(4,5))
let(x=acosh(cosh(e4)*cosh(e4/2)-sinh(e4)*sinh(e4/2)*cos(2*pi/5)))
let(al=asin(sinh(e4/2)*sin(2*pi/5)/sinh(x)))
tile(e4,al,2*x,2*pi/5-al,e4,2*pi/5,e4,4*pi/5)
tile(e4,al,2*x,2*pi/5-al,e4,2*pi/5,e4,4*pi/5)
tile(e4,al,2*x,2*pi/5-al,e4,2*pi/5,e4,4*pi/5)
conway("(0 2)(1)[3 2'](0' 2'')(1' 1'')(3' 0'')(3'')")
